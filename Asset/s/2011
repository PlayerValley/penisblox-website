r()).Color;newButton.Name=tostring(BrickColor.new(n))n=n+1;if p==1 then newButton.Position=UDim2.new(0.08,0,o/m+l,0)elseif p==2 then newButton.Position=UDim2.new(0.29,0,o/m+l,0)elseif p==3 then newButton.Position=UDim2.new(0.5,0,o/m+l,0)elseif p==4 then newButton.Position=UDim2.new(0.71,0,o/m+l,0)end;newButton.Parent=h end end;local q=h:GetChildren()for o=1,#q do q[o].MouseButton1Click:connect(function()fadeInButton(q[o])end)end;local r=e:clone()r.RobloxLocked=true;r.Parent=h;local s=Instance.new("BoolValue")s.RobloxLocked=true;s.Name="ClosedState"s.Parent=r;setUpCloseButtonState(r)local t=Instance.new("ImageLabel")local u=a.BuildTools.Frame.PropertyTools.MaterialSelector;t.RobloxLocked=true;t.Name="MaterialMenu"t.Position=UDim2.new(-4,0,-3,0)t.Size=UDim2.new(2.5,0,6.5,0)t.BackgroundTransparency=1;t.ZIndex=2;t.Image=c.."MaterialMenu.png"t.Parent=u;local v={"Plastic","Wood","Slate","CorrodedMetal","Ice","Grass","Foil","DiamondPlate","Concrete"}local w={}local x=Instance.new("ImageButton")x.RobloxLocked=true;x.BackgroundTransparency=1;x.Size=UDim2.new(0.400000003,0,0.16,0)x.ZIndex=2;k.Parent=x;local y=1;function getTextureAndName(f)if y>#v then f:remove()return false end;f.Image=c..v[y]..".png"f.Name=v[y]y=y+1;return true end;local z=0.10;local A=0.07;for o=1,5 do for p=1,2 do local f=x:clone()f.RobloxLocked=true;f.Position=UDim2.new((p-1)/2.2+A,0,z+(o-1)/5.5,0)if getTextureAndName(f)then f.Parent=t else f:remove()end;table.insert(w,f)end end;for o=1,#w do w[o].MouseButton1Click:connect(function()fadeInButton(w[o])end)end;local B=e:clone()B.RobloxLocked=true;B.Size=UDim2.new(0.2,0,0.08,0)B.Parent=t;local s=Instance.new("BoolValue")s.RobloxLocked=true;s.Name="ClosedState"s.Parent=B;setUpCloseButtonState(B)local C=Instance.new("ImageLabel")local D=a.BuildTools.Frame.PropertyTools.InputSelector;C.RobloxLocked=true;C.Name="SurfaceMenu"C.Position=UDim2.new(-2.6,0,-4,0)C.Size=UDim2.new(2.5,0,5.5,0)C.BackgroundTransparency=1;C.ZIndex=2;C.Image=c.."SurfaceMenu.png"C.Parent=D;